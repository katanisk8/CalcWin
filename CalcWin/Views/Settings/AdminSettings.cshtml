@model AdminSettingsViewModel
@{
    ViewData["Title"] = "Admin Settings";
    ViewData.AddActivePage(SettingsNavPages.AdminSettings);
}

<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Admin Settings
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-6">
                            <form role="form">
                                <div class="form-group">
                                    <label>Default application data</label>
                                    <input type="file">
                                </div>
                                <button type="submit" class="btn btn-default">Load</button>
                                <button type="reset" class="btn btn-default">Reset</button>
                            </form>
                        </div>
                        <div class="col-lg-6">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
